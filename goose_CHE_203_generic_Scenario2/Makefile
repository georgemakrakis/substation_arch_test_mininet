LIBIEC_HOME=/home/mininet/libiec61850/

PROJECT_BINARY_NAME_1 = goose_CHE_203_generic_Scenario2
PROJECT_BINARY_NAME_2 = goose_CHE_203_RTAC_Scenario2

PROJECT_SOURCES_1 = goose_CHE_203_generic_Scenario2.c
PROJECT_SOURCES_2 = goose_CHE_203_RTAC_Scenario2.c

include $(LIBIEC_HOME)/make/target_system.mk
include $(LIBIEC_HOME)/make/stack_includes.mk

all:	$(PROJECT_BINARY_NAME_1) $(PROJECT_BINARY_NAME_2) 

include $(LIBIEC_HOME)/make/common_targets.mk

$(PROJECT_BINARY_NAME_1):	$(PROJECT_SOURCES_1) $(LIB_NAME)
	$(CC) $(CFLAGS) $(LDFLAGS) -o $(PROJECT_BINARY_NAME_1) $(PROJECT_SOURCES_1) $(INCLUDES) $(LIB_NAME) $(LDLIBS) -lm

$(PROJECT_BINARY_NAME_2):	$(PROJECT_SOURCES_2) $(LIB_NAME)
	$(CC) $(CFLAGS) $(LDFLAGS) -o $(PROJECT_BINARY_NAME_2) $(PROJECT_SOURCES_2) $(INCLUDES) $(LIB_NAME) $(LDLIBS) -lm

clean:
	rm -f $(PROJECT_BINARY_NAME_1)
	rm -f $(PROJECT_BINARY_NAME_2)
